<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="cal.venue">
<body>

<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main">

        <div tal:replace="structure provider:plone.abovecontenttitle" />

        <h1 class="documentFirstHeading">
            <span metal:use-macro="python:context.widget('title', mode='view')" />
        </h1>

        <div tal:replace="structure provider:plone.belowcontenttitle" />

        <div tal:replace="structure provider:plone.abovecontentbody" />

        <div metal:use-macro="python:context.widget('street1', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('street2', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('zipCode', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('city', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('state', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('contactName', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('contactEmail', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('contactPhone', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('url', mode='view')" /><br />
        <div metal:use-macro="python:context.widget('cityMap', mode='view')" /><br />

        <div tal:condition="text" metal:use-macro="python:context.widget('text', mode='view')" /><br />

        <div metal:use-macro="context/document_relateditems/macros/relatedItems">
            show related items if they exist
        </div>

        <div tal:replace="structure provider:plone.belowcontentbody" />

    </tal:main-macro>
</metal:main>

</body>
</html>
