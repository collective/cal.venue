<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="cal.venue">
<body>

<metal:content-core fill-slot="content-core">
    <metal:content-core define-macro="content-core">

        <tal:field metal:use-macro="python:context.widget('street1', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('street2', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('zipCode', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('city', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('state', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('contactName', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('contactEmail', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('contactPhone', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('url', mode='view')" /><br />
        <tal:field metal:use-macro="python:context.widget('cityMap', mode='view')" /><br />

        <tal:field metal:use-macro="python:context.widget('text', mode='view')" /><br />

    </metal:content-core>
</metal:content-core>

</body>
</html>
